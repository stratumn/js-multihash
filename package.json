{
  "name": "multihashes",
  "version": "0.4.13",
  "description": "multihash implementation",
  "main": "lib/index.js",
  "scripts": {
    "lint": "aegir lint && npm run type-check",
    "test:browser": "npm run build && aegir test --target browser",
    "test:node": "npm run build:node && aegir test --target node",
    "type-check": "flow check",
    "build": "npm run build:node && BABEL_ENV=umd aegir build",
    "test": "npm run build:node && aegir test",
    "docs": "aegir docs",
    "release": "aegir release --docs",
    "release-minor": "aegir release --type minor --docs",
    "release-major": "aegir release --type major --docs",
    "coverage": "aegir coverage",
    "coverage-publish": "aegir coverage --provider coveralls",
    "build:types": "flow-copy-source --verbose src lib",
    "build:lib": "babel --out-dir lib src",
    "build:node": "npm run build:types && npm run build:lib",
    "start": "flow-copy-source --watch --verbose src lib & babel --watch --out-dir lib src"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/multiformats/js-multihash.git"
  },
  "keywords": [
    "multihash"
  ],
  "author": "Juan Benet <juan@benet.ai> (http://juan.benet.ai/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/multiformats/js-multihash/issues"
  },
  "dependencies": {
    "bs58": "^4.0.1",
    "varint": "^5.0.0"
  },
  "devDependencies": {
    "aegir": "git+https://github.com/ipfs/aegir#flow",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-flow-node": "2.0.1",
    "buffer-equal": "1.0.0",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.2.0",
    "pre-commit": "^1.2.2"
  },
  "contributors": [
    "Alan Shaw <alan@tableflip.io>",
    "David Dias <daviddias.p@gmail.com>",
    "Donald Tsang <brandonknlee@gmail.com>",
    "Fil <fil@rezo.net>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "Juan Benet <juan@benet.ai>",
    "Pedro Teixeira <i@pgte.me>",
    "Richard Littauer <richard.littauer@gmail.com>",
    "Trond Arne Br√•then <tabrath@gmail.com>",
    "Victor Bjelkholm <victorbjelkholm@gmail.com>",
    "Volker Mische <volker.mische@gmail.com>",
    "William Cotton <williamcotton@gmail.com>",
    "greenkeeper[bot] <greenkeeper[bot]@users.noreply.github.com>",
    "kumavis <aaron@kumavis.me>",
    "nginnever <ginneversource@gmail.com>",
    "npm-to-cdn-bot (by Forbes Lindesay) <npmcdn-to-unpkg-bot@users.noreply.github.com>"
  ]
}
